{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@utils/*": ["./src/utils/*"],
      "@store/*": ["./src/store/*"],
      "@actions/*": ["./src/store/actions/*"],
      "@services/*": ["src/services/*"],
      "@components/*": ["./src/components/*"],
      "@css/*": ["./src/assets/css/*"],
      "@js/*": ["./src/assets/js/*"],
      "@img/*": ["./src/assets/img/*"],
      "@config/*": ["./src/config/*"],
      "@assets/*": ["./src/assets/*"],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}
